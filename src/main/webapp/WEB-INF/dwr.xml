<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN"
    "http://getahead.org/dwr/dwr20.dtd">

<dwr>
  <allow>
    <create creator="new" javascript="DisplayTagService">
        <param name="class" value="org.ncibi.mimiweb.dwr.DisplayTagService"/>
    </create>

    <create creator="new" javascript="GeneSelectionService">
        <param name="class" value="org.ncibi.mimiweb.dwr.SelectionServiceForGeneQueryPage"/>
    </create>

    <create creator="new" javascript="IbisSearchService">
        <param name="class" value="org.ncibi.ibis.dwr.service.IbisSearchService"/>
    </create>

    <create creator="new" javascript="IbisFilterService">
        <param name="class" value="org.ncibi.ibis.dwr.service.IbisFilterService"/>
    </create>

    <create creator="new" javascript="CountWebService">
        <param name="class" value="org.ncibi.mimiweb.dwr.CountWebService"/>
    </create>

    <convert match="org.ncibi.ibis.data.GeneBean" converter="bean"/>
    <convert match="org.ncibi.ibis.attribute.GeneListAttributesBean" converter="bean"/>
    <convert match="org.ncibi.ibis.attribute.GeneListAttributesBean" converter="bean"/>
    <convert match="org.ncibi.ibis.attribute.Attribute" converter="bean"/>
    <convert match="org.ncibi.ibis.data.IbisResultsBean" converter="bean"/>
  </allow>
</dwr>

